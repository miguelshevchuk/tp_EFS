    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-standard": "^5.0.0",
    "husky": "^7.0.1",
    "nodemon": "^2.0.7",
    "prettier": "2.3.2",
    "pretty-quick": "^3.1.1",
